// Example IDL file for our Button's schema.

namespace MyButtons.Sample;

table Frame {
  buttons:[Button];
}

struct Color {
    r:int;
    g:int;
    b:int;
    a:int = 255;
}

struct Point {
  x:int;
  y:int;
}

table Button {
  id:int;
  priority:short = 1;
  label:string;
  start:Point;
  end:Point;
  color:Color;
  button_contour:bool = 1;
  type_line:bool = 0;
}

root_type Frame;
